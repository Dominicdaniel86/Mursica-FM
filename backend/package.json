{
  "name": "spotify-session-app-nodejs-backend",
  "version": "0.2.0",
  "description": "A Node.js backend for hosting a third-party Spotify session app.",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --legacy-watch --watch ./src --ext ts --exec 'node --inspect=0.0.0.0:9229 --nolazy --loader ts-node/esm' ./src/server.ts",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dominicdaniel86/Spotify-Session-App"
  },
  "keywords": [
    "typescript",
    "backend",
    "spotify",
    "session",
    "nodejs"
  ],
  "author": "Dominic Daniel Kraemer",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "esm": "^3.2.25",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
